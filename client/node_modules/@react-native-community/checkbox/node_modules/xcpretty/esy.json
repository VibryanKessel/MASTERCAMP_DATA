{
  "name": "xcpretty",
  "version": "0.2.8001",
  "esy": {
    "build": [
      ["cp", "-r", "assets", "#{self.install}"],
      ["cp", "-r", "bin", "#{self.install}"],
      ["cp", "-r", "features", "#{self.install}"],
      ["cp", "-r", "lib", "#{self.install}"],
      ["cp", "-r", "spec", "#{self.install}"],
      ["cp", "-r", "Gemfile", "#{self.install}"]
    ],
    "install": [],
    "buildsInSource": true,
    "exportedEnv": {
      "GEM_PATH": {
        "val": "#{self.install : $GEM_PATH}",
        "scope": "local"
      }
    }
  }
}
